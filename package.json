{
  "name": "github-fetch-starter",
  "version": "1.0.0",
  "description": "A github starter fetcher.",
  "repository": "vdegenne/github-fetch-starter",
  "license": "BSD-3-Clause",
  "bin": {
    "github-fetch-starter": "bin/github-fetch-starter"
  },
  "scripts": {
    "clean": "mkdir -p lib && rm -r lib",
    "build": "yarn clean && tsc",
    "test": "yarn build && mocha",
    "test:watch": "watchy -w src/ -- yarn test",
    "debug:mocha": "yarn build && node --inspect-brk=14349 node_modules/.bin/_mocha",
    "debug:bin": "yarn build && node --inspect-brk=14350 ./bin/github-fetch-starter -u vdegenne -n myapp node-cli-starter",
    "format": "find src -iname '*.ts' -exec clang-format --style=file -i {} \\+"
  },
  "keywords": [
    "github",
    "fetcher",
    "fetch",
    "start",
    "starter"
  ],
  "dependencies": {
    "@types/node": "^9.6.1",
    "@types/opn": "^5.1.0",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.4",
    "github-latest-release": "^0.1.1",
    "isdir-async": "^1.0.0",
    "plylog": "^0.4.0",
    "resolve": "^1.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.0.0",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "clang-format": "^1.0.41-c",
    "mocha": "^5.0.5",
    "opn": "^5.3.0",
    "source-map-support": "^0.5.4",
    "tsc-then": "^1.1.0",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1",
    "watchy": "^0.8.0"
  },
  "typings": "./lib/github-fetch-starter.d.ts"
}
